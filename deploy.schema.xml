<deployment project="GSA" version = "0.1">
	<!-- Complex Schema constructs -->
	<apps>
		<app name="site-search" id="1"/>
		<app name="course-search" id="2"/>
		<app name="zendesk" id="3"/>
	</apps>
	<!-- 
	frontends 
	follows native naming convention of GSA
	advised by OOBE, which is that the frontend
	name matches the filename of the XSLT proxysheet
	-->
	<frontends>
		<frontend name="env_variables" appRef="1"/>
		<frontend name="scu_frontend" appRef="1"/>
		<frontend name="scu_snippets" appRef="1"/>
		<frontend name="course_search" appRef="2"/>
		<frontend name="servicedesk_frontend" appRef="3"/>
	</frontends>
	<!-- 
	reports
	these report children are converted to an ArrayList of strings
	based on the collections attr
	-->
	<reports>
		<report collection="scu" appRef="1"/>
		<report collection="squiz-courses" appRef="1"/>
		<report collection="squiz-courses" appRef="2"/>
	</reports>
	<!-- Basic Schema constructs -->
	<!-- 
	variables 
	simple map schema used to replace deployment 
	driven parameters to environment variables
	-->
	<variables>
		<variable match="%env_param%" envKey="env.param"/>
	</variables>
	<!-- 
	tasks 
	used to map stable tasks in the SCU API Stack
	-->
	<tasks>
		<task name="Report" lifecycle="stable"/>
		<task name="FrontEndImport" lifecycle="stable"/>
	</tasks>
	<!--
	releases
	seeded by bamboo as a java release directive
	-->
	<releases>
		<release/>
	</releases>
</deployment>